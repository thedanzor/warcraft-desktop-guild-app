{
	"name": "Warcraft-guild-manager",
	"version": "0.3.0",
	"description": "A simple desktop application to browse and manage guilds",
	"main": "src/build/init.js",
	"scripts": {
		"clear": "rm ./build/app.js && rm ./build/app.js.map",
		"webpack": "npm run && webpack --progress --optimize-minimize --colors",
		"start": "npm run webpack && npm run clear && electron ./build/init.js",
		"load": "electron ./build/init.js"
	},
	"ava": {
		"verbose": true,
		"require": [
			"babel-register",
			"./test/helpers/setup-browser-env.js"
		],
		"babel": "inherit",
		"env": {
			"development": {
				"sourceMaps": "inline"
			}
		}
	},
	"author": "Scott Jones <scott@thedanzorlabs.com>",
	"homepage": "https://the-danzor.deviantart.com",
	"private": true,
	"devDependencies": {
		"autoprefixer": "^7.1.2",
		"ava": "^0.20.0",
		"babel-core": "^6.25.0",
		"babel-loader": "^7.1.0",
		"babel-plugin-syntax-async-functions": "^6.13.0",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-es2015-spread": "^6.22.0",
		"babel-plugin-transform-function-bind": "^6.22.0",
		"babel-plugin-transform-object-rest-spread": "^6.23.0",
		"babel-plugin-transform-regenerator": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.23.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"browser-env": "^3.2.0",
		"clean-terminal-webpack-plugin": "^1.0.0",
		"command-line-args": "^4.0.6",
		"css-loader": "^0.28.4",
		"electron": "~1.7.8",
		"eslint": "^4.7.0",
		"eslint-config-airbnb-base": "^12.0.0",
		"eslint-plugin-ava": "^4.2.2",
		"eslint-plugin-import": "^2.7.0",
		"extract-text-webpack-plugin": "^2.1.2",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.28.0",
		"json-loader": "^0.5.4",
		"node-sass": "^4.5.3",
		"nyc": "^11.2.1",
		"postcss-loader": "^2.0.6",
		"prettier": "^1.4.4",
		"request": "^2.71.0",
		"sass-loader": "^6.0.6",
		"sockjs-client": "^1.1.4",
		"style-loader": "^0.18.2",
		"uglify-js": "^2.8.23",
		"url-loader": "^0.6.2",
		"webpack": "^3.4.1",
		"webpack-dev-server": "^2.5.0"
	},
	"env": {
		"development": {
			"sourceMaps": "inline"
		}
	},
	"dependencies": {
		"nedb": "^1.8.0",
		"node-fetch": "^2.6.7"
	}
}
